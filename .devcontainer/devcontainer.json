{
    "name": "AdventOfCode C++ Dev Environment",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "service": "cpp",
    "workspaceFolder": "/workspace",
    "shutdownAction": "stopCompose",
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.defaultProfile.linux": "bash"
            },
            "extensions": [
                "ms-vscode.cpptools",
                "ms-vscode.cpptools-themes",
                "ms-vscode.cmake-tools",
                "xaver.clang-format",
                "twxs.cmake"
            ]
        }
    }
    ,"remoteUser": "developer"
}
